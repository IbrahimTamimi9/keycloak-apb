apiVersion: v1
kind: Secret
metadata:
  name: keycloak-public-client
  namespace: {{ namespace }}
stringData:
  installation: "{{ installationbearer | regex_replace('(u\'|\')', '\\\"') }}"
  keycloak_host: http://{{ keycloak_route.stdout }}
  client_id: {{ generated_username.stdout }}
  client_secret: {{ generated_password.stdout }}
      